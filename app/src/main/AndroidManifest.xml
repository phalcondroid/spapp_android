        <activity
            android:name="com.msg.spapp.TrainerActivity"
            android:label="@string/title_activity_trainer" >
        </activity>
        <activity
            android:name="com.msg.spapp.SummaryActivity"
            android:label="@string/title_activity_summary" >
        </activity>
        <activity
            android:name="com.msg.spapp.RatingActivity"
            android:label="@string/title_activity_rating" >
        </activity>
        <activity
            android:name="com.msg.spapp.PersonalInfoActivity"
            android:label="@string/title_activity_first_register" >
        </activity>
        <activity
            android:name="com.msg.spapp.DetailServiceActivity"
            android:label="@string/title_activity_detail_service" >
